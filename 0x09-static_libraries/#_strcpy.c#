#include "main.h"

char *_strcpy(char *dest, char *src)
{
  char *p = dest;
  while (*src)
    *p++ = *src++;
  *p = '\0';
  return dest;
}
char *_strcpy(char *dest, char *src) {
    int i = 0;
    while (src[i] != '\0') {
        dest[i] = src[i];
        i++;
    }
    dest[i] = '\0';
    return dest;
}
